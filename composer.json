{
    "name": "gamegos/nosql",
    "description": "Library to store data in NoSql databases",
    "keywords": [
        "nosql",
        "key value",
        "cache"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "gamegos/events": "~1"
    },
    "autoload": {
        "psr-4": {
            "Gamegos\\NoSql\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8 - 5.7",
        "gamegos/php-code-sniffer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Gamegos\\NoSql\\Tests\\": "tests/"
        }
    },
    "config": {
        "cache-dir": ".tmp/composer"
    },
    "authors": [
        {
            "name": "Safak Ozpinar",
            "email": "safak@gamegos.com"
        }
    ]
}
